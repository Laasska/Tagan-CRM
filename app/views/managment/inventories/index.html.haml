%div.container.mt50
  %div.row
    %table.table.table-bordered.col-md-12.table-hover
      %thead.table-inverse
        %tr
          %th #
          %th Title
          %th Quantity
          %th Amount of free
      %tbody
        - i = 0
        - @groupedList.each do |key|
          %tr
            %th{:scope => "row"}
              = i += 1
              %td= key
              %td= @counts[key]
              %td= @countsfree[key]



.btn-group{"aria-label" => "...", :role => "group"}
  %a.btn{:onclick => 'addInventories()'} New Inventory

= render template: "managment/inventories/new"